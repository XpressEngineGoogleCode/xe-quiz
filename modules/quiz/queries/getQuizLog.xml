<?xml version="1.0" encoding="UTF-8"?>
<query id="getQuizLog" action="select">
    <tables>
        <table name="quiz_log" />
    </tables>
    <columns>
        <column name="*" />
    </columns>
    <conditions>
        <condition operation="equal" column="module_srl" var="module_srl" />
        <condition operation="equal" column="user_identifier" var="user_identifier" pipe="and" />
    </conditions>
</query>

