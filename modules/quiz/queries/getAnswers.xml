<?xml version="1.0" encoding="UTF-8"?>
<query id="getAnswers" action="select">
    <tables>
        <table name="quiz_answers" />
    </tables>
    <columns>
        <column name="*" />
    </columns>
    <conditions>
        <condition operation="equal" column="module_srl" var="module_srl" />
    </conditions>
    <navigation>
        <index var="sort_index" default="list_order" order="asc" />
    </navigation>
</query>

