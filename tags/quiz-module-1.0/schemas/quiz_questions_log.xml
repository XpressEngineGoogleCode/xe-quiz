<table name="quiz_questions_log">
    <column name="module_srl" type="number" size="11" notnull="notnull" />
    <column name="member_srl" type="number" size="11" notnull="notnull" index="idx_member_srl" />
    <column name="question_srl" type="number" size="11" notnull="notnull"  />    
    <column name="ipaddress" type="varchar" size="128" notnull="notnull" index="idx_ipaddress" />
    <column name="user_identifier" type="varchar" size="128" notnull="notnull" index="idx_user_identifier" />
    <column name="regdate" type="date" index="idx_regdate" />
    <column name="start_date" type="date" />
    <column name="end_date" type="date" />
    <column name="answer" type="varchar" size="255" notnull="notnull" />
    <column name="is_active" type="char" size="1" default="Y" notnull="notnull" />
    <column name="is_correct" type="char" size="1" />
    <column name="weight" type="number" size="11" notnull="notnull" default="0" />
    <column name="attempt" type="number" size="11" notnull="notnull" default="1" />
</table>
