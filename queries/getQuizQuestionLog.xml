<?xml version="1.0" encoding="UTF-8"?>
<query id="getQuizQuestionLog" action="select">
    <tables>
        <table name="quiz_questions_log" />
    </tables>
    <columns>
        <column name="*" />
    </columns>
    <conditions>
        <condition operation="equal" column="question_srl" var="question_srl" />
        <condition operation="equal" column="user_identifier" var="user_identifier" pipe="and" />
        <condition operation="equal" column="is_active" var="is_active" pipe="and" />
    </conditions>
</query>

