<filter name="start_question" module="quiz" act="procStartQuestion"	>
    <form />
    <parameter>
    	<param name="module_srl" target="module_srl" />
    	<param name="question_srl" target="question_srl" />
    </parameter>
    <response callback_func="completeStartQuestion">
        <tag name="error" />
        <tag name="message" />
        <tag name="question_srl" />        
        <tag name="login_url" />
    </response>
</filter>
