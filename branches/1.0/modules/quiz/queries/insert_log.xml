<query id="insert_log" action="insert">
    <tables>
        <table name="quiz_log" />
    </tables>
    <columns>       
	    <column name="module_srl" var="module_srl" filter="number" notnull="notnull" />
	    <column name="member_srl" var="member_srl" filter="number" default="0" />
	    <column name="ipaddress" var="ipaddress" default="ipaddress()" />
	    <column name="user_identifier" var="user_identifier" notnull="notnull" />
	    <column name="regdate" var="regdate" default="curdate()" />
	    <column name="score" var="score" filter="number" default="0" />
	    <column name="total_time" var="total_time" filter="number" default="0" />
	    <column name="start_date" var="start_date" />
	    <column name="end_date" var="end_date" />
    </columns>
</query>